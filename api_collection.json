				"header": [],
				"body": {
					"mode": "raw",
					"raw": "{\n    \"type\": \"OracleProvisionParameters\",\n    \"container\": {\n        \"type\": \"OracleDatabaseContainer\",\n        \"name\": \"apivdb\",\n        \"group\": \"GROUP-1\"\n    },\n    \"source\": {\n        \"type\": \"OracleVirtualSource\",\n        \"mountBase\": \"/mnt/provision\",\n        \"configParams\": {\n            \"processes\": \"300\",\n            \"nls_language\": \"'AMERICAN'\",\n            \"pga_aggregate_target\": \"524288000\",\n            \"sga_target\": \"1577058304\",\n            \"nls_territory\": \"'AMERICA'\",\n            \"compatible\": \"'11.2.0.4'\",\n            \"remote_login_passwordfile\": \"'EXCLUSIVE'\",\n            \"open_cursors\": \"300\"\n        },\n        \"nodeListeners\": [],\n        \"operations\": {\n            \"type\": \"VirtualSourceOperations\",\n            \"configureClone\": [],\n            \"preRefresh\": [],\n            \"postRefresh\": [],\n            \"preRollback\": [],\n            \"postRollback\": [],\n            \"preSnapshot\": [],\n            \"postSnapshot\": [],\n            \"preStart\": [],\n            \"postStart\": [],\n            \"preStop\": [],\n            \"postStop\": []\n        },\n        \"redoLogSizeInMB\": 1024,\n        \"redoLogGroups\": 3,\n        \"archivelogMode\": true,\n        \"customEnvVars\": [],\n        \"allowAutoVDBRestartOnHostReboot\": true\n    },\n    \"sourceConfig\": {\n        \"type\": \"OracleSIConfig\",\n        \"name\": \"apivdb\",\n        \"linkingEnabled\": false,\n        \"repository\": \"ORACLE_INSTALL-7\",\n        \"services\": [],\n        \"databaseName\": \"apivdb\",\n        \"uniqueName\": \"apivdb\",\n        \"instance\": {\n            \"type\": \"OracleInstance\",\n            \"instanceNumber\": 1,\n            \"instanceName\": \"apivdb\"\n        }\n    },\n    \"timeflowPointParameters\": {\n        \"type\": \"TimeflowPointSemantic\",\n        \"location\": \"LATEST_POINT\",\n        \"container\": \"ORACLE_DB_CONTAINER-1\"\n    },\n    \"masked\": false,\n    \"openResetlogs\": true,\n    \"physicalStandby\": false,\n    \"newDBID\": false\n}",
					"options": {
						"raw": {
							"language": "json"
						}
					}
				},
				"url": {
